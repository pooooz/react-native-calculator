{
  "name": "react-native-calculator",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "e2e:android": "detox build -c android && detox test -c android",
    "test:android": "detox test -c android",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "e2e:android.att.debug": "detox build --configuration android.att.debug & detox test --configuration android.att.debug",
    "e2e:android.att.release": "detox build --configuration android.att.release & detox test --configuration android.att.release",
    "e2e:android.emu.debug": "detox build --configuration android.emu.debug & detox test --configuration android.emu.debug",
    "e2e:android.emu.release": "detox build --configuration android.emu.release & detox test --configuration android.emu.release",
    "test:android.att.debug": "detox test --configuration android.att.debug",
    "test:android.att.release": "detox test --configuration android.att.release",
    "test:android.emu.debug": "detox test --configuration android.emu.debug",
    "test:android.emu.release": "detox test --configuration android.emu.release",
    "e2e:ios.sim.debug": "detox build --configuration ios.sim.debug & detox test --configuration ios.sim.debug",
    "e2e:ios.sim.release": "detox build --configuration ios.sim.release & detox test --configuration ios.sim.release",
    "test:ios.sim.debug": "detox test --configuration ios.sim.debug",
    "test:ios.sim.release": "detox test --configuration ios.sim.release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-navigation/material-top-tabs": "^6.3.0",
    "@react-navigation/native": "^6.0.13",
    "react": "18.1.0",
    "react-native": "0.70.4",
    "react-native-pager-view": "^6.0.2",
    "react-native-reanimated": "^2.12.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-tab-view": "^3.3.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@tsconfig/react-native": "^2.0.2",
    "@types/detox": "^18.1.0",
    "@types/jasmine": "^4.3.0",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "detox": "^20.0.3",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-detox": "^1.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
